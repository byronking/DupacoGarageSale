<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/bootstrap/css")
    @Styles.Render("~/Content/timepicker/css")
    @Styles.Render("~/Content/lightbox/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @*<script src="//code.jquery.com/jquery-1.8.2.js"></script>*@
    <script src="~/Scripts/jquery-ui-1.11.3.js"></script>
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/js")
    @Scripts.Render("~/bundles/timepicker")
    @Scripts.Render("~/bundles/bootstrap")
    <link rel="stylesheet" href="//code.jquery.com/ui/1.11.3/themes/base/jquery-ui.css">
    @Scripts.Render("~/bundles/lightboxjs")
    
</head>
<body>
    @if (Session["UserSession"] != null)
    {
        @Html.Partial("~/Views/Shared/_HomeNavLoggedIn.cshtml")
    }
    else
    {
        @Html.Partial("~/Views/Shared/_TopNavigation.cshtml")
    }
    
    @RenderBody()

    @Html.Partial("~/Views/Shared/_Footer.cshtml")

    @RenderSection("scripts", required: false)
</body>
</html>
