<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/bootstrap/css")
    @Styles.Render("~/Content/timepicker/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/js")
    @Scripts.Render("~/bundles/timepicker")
    @Scripts.Render("~/bundles/bootstrap")
</head>
<body>
    @if (Session["UserSession"] != null)
    {
        @Html.Partial("~/Views/Shared/_HomeNavLoggedIn.cshtml")
    }
    else
    {
        @Html.Partial("~/Views/Shared/_TopNavigation.cshtml")
    }
    
    @*<div class="container" style="margin-top:60px;">*@
    <div class="container" style="margin-top:40px;">
        <div id="bg">
          <img src="~/Content/images/Insulators-3080-Karen-St-DBQ.jpg" alt="">
        </div>

        @RenderBody()
    </div>

    @RenderSection("scripts", required: false)
</body>
</html>
